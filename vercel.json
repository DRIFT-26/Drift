{
  "crons": [
    {
      "path": "/api/jobs/daily",
      "schedule": "0 14 * * *",
      "headers": {
        "x-cron-secret": "${CRON_SECRET}"
      }
    },
    {
      "path": "/api/jobs/weekly",
      "schedule": "0 15 * * 1",
      "headers": {
        "x-cron-secret": "${CRON_SECRET}"
      }
    }
  ]
}